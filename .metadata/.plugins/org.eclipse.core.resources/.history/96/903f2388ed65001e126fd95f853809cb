package javaweek5;

public class USBMemTest {
	
	int cap;
	String color;
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		public USBMem(int c, String cir) {
			cap = c;
			color = cir;
		}
		void USBAttrShow() {
			System.out.println("용량이 "+cap+"G 색상이 "+color+"인 USB : ");
		}
		void Memwrite() {
			USBAttrShow();
			System.out.println("데이터를 메모리에 씁니다. ");
		}
		void MemRead() {
			USBAttrShow();
			System.out.println("데이터를 메모리에서 읽었습니다 ");
		}
	}

}
